{
  "name": "pastebin-lite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev --no-turbo",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
